{
	"name": "@embedz/svelte",
	"version": "1.0.16",
	"description": "Easy, dependency free embeds for Svelte and Vue.",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/embedz/embedz.git"
	},
	"bugs": {
		"url": "https://github.com/embedz/embedz/issues"
	},
	"homepage": "https://github.com/embedz/embedz",
	"author": {
		"name": "embedz",
		"url": "https://github.com/embedz/embedz"
	},
	"license": "MIT",
	"private": false,
	"keywords": [
		"javascript",
		"components",
		"youtube",
		"vimeo",
		"typescript",
		"vue",
		"svelte",
		"embeds",
		"iframe",
		"embedz"
	],
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run prepack",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "svelte-kit sync && svelte-package && publint",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"main": "./dist/index.js",
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.7",
		"@eslint/js": "^9.21.0",
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/kit": "^2.17.2",
		"@sveltejs/package": "^2.3.10",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/svelte": "^5.2.7",
		"eslint": "^9.21.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^2.46.1",
		"globals": "^16.0.0",
		"jsdom": "^26.0.0",
		"prettier": "^3.5.2",
		"prettier-plugin-svelte": "^3.3.3",
		"publint": "^0.3.6",
		"svelte": "^5.20.4",
		"svelte-check": "^4.1.4",
		"typescript": "^5.7.3",
		"typescript-eslint": "^8.25.0",
		"vite": "^6.1.1",
		"vitest": "^3.0.7"
	}
}
